typedef Bool
{
 | F
 | T
 }

define &(a: Bool, b: Bool) : Bool
{
 switch a
 {
  | F { F }
  | T { b }
  }
 }

define |(a: Bool, b: Bool) : Bool
{
 switch a
 {
  | T { T }
  | F { b }
  }
 }


printRaw(&(T, |(F,T,),))
printRaw((switch T {| T { & } | F { | } })(T, F))
printRaw(F & T | T)
printRaw(F & T | T & F | T)

;; F & T | T = F & (T | T)
;; (&(T, |(F,T,),)) = T
;; ((switch T {| T { & } | F { | } })(T, F)) = F

printRaw(identical(Bool, T, F))
