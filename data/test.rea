typedef Bool
{
 | F
 | T
 }

define &(a: Bool, b: Bool) : Bool
{
 switch a
 {
  | F { F }
  | T { b }
  }
 }

define |(a: Bool, b: Bool) : Bool
{
 switch a
 {
  | T { T }
  | F { b }
  }
 }

print(F&T | T)
print((F & T) | T)
print(F & (T | T))

print(F & T | T = F & (T | T))

